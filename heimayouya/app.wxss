@import './styles/iconfont';

/* 在微信小程序中不支持* */
page,
view,
text,
swiper,
swiper-item,
image,
navigator {
    padding: 0;
    margin: 0;
    box-sizing: border-box;
}
/* 主题颜色 */
page {
    /* 定义主体颜色 */
    --themeColor: #eb4450;
    /* 定义统一字体大小,假设设计稿是375px */
    font-size: 28rpx;
}
image {
    width: 100%;
}
